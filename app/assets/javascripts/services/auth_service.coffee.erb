angular.module('Infoluence').service 'AuthService', [

	'$http'
 'CookieService'
 ($http,CookieService) ->

  @isSignedIn = =>
   if CookieService.getUserInfo() != undefined then true else false



	 @signout = ->
		 req =
		  method : "put"
		  url : "/user/logout.json"
		  dataType : "json"
		  # params:
		  #  authenticity_token:$("meta[name='csrf-token']").attr("content")

		 promise = $http(req)
		 promise

		
  return

]