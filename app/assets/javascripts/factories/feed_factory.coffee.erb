
angular.module('Infoluence').factory "FeedFactory", [

 'FeedService'

 (FeedService) ->

  o =
   feed: []
   pageNum: 0

  o.getFeed = (type) =>

   o.pageNum++
   FeedService.getFeed(type,o.pageNum).then (res) =>

    data = res.data

    data.forEach (post) =>
     # comment.timestamp  = comment.time
     post.publishTimeString = moment(post.publishTime * 1000).format("L, h:mm:ss a")
     post.createTimeString = moment(post.createTime * 1000).format("L, h:mm:ss a")
     
     o.feed.push post

  o.clear = =>
   o.feed = []
   pageNum:0



  o

]
